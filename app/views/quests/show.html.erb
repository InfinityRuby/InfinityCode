          <button class="quest-tab-button active questbtn"  data-id="question">Question</button>
          <button class="quest-tab-button questbtn" data-id="prompt" >Prompt</button>
        </div>
        <div class="quest-contentWrapper">
          <div class="quest-content active" id="question">
